
server {
	listen 80;
	server_name  localhost www.localhost admin www.admin;
	proxy_set_header backdoor sys;

	
	location  / {
		client_max_body_size    1000m;
		proxy_read_timeout 10000s;
		proxy_pass http://127.0.0.1:8080;
	}
			 
	location  ~ ^/(myDeviceCloundNew|mqsas|mqsasdata|mqsasABTest|knowledge|whiteList|mqsasback|test1|myDeviceClound)/ {
		client_max_body_size    1000m;
		proxy_read_timeout 10000s;
		proxy_pass http://sys.pt.miui.com;
	}
			
}
			